{
  "compilerOptions": {
    "experimentalDecorators": true,
    "strict": true,
    "declaration": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "src/components/miscQuiz/quizpage/quiz/validate.js",
    "src/components/miscQuiz/quizpage/quiz/store.js",
    "src/components/miscQuiz/quizpage/quiz/QuizFormResult.js",
    "src/components/miscQuiz/quizpage/quiz/Quiz.tsx",
    "src/components/miscQuiz/quizpage/quiz/QuizForm.js",
    "src/components/miscQuiz/quizpage/quiz/index.js",
    "server.ts",
    "src/components", "src/custom.d.ts"
  ]
}
